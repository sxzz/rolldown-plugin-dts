{
  "name": "rolldown-plugin-dts",
  "version": "0.13.0",
  "packageManager": "pnpm@10.11.0",
  "description": "A Rolldown plugin to bundle dts files",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/sxzz/rolldown-plugin-dts#readme",
  "bugs": {
    "url": "https://github.com/sxzz/rolldown-plugin-dts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/rolldown-plugin-dts.git"
  },
  "author": "三咲智子 Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "rolldown": "^1.0.0-beta.8-commit.2a5c6a6",
    "typescript": "^5.0.0",
    "vue-tsc": "~2.2.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "vue-tsc": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/generator": "^7.27.1",
    "@babel/parser": "^7.27.2",
    "@babel/types": "^7.27.1",
    "ast-kit": "^2.0.0",
    "birpc": "^2.3.0",
    "debug": "^4.4.1",
    "dts-resolver": "^2.0.1",
    "get-tsconfig": "^4.10.0"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.0.1",
    "@sxzz/prettier-config": "^2.2.1",
    "@sxzz/test-utils": "^0.5.6",
    "@types/babel__generator": "^7.27.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.15.17",
    "@volar/typescript": "^2.4.13",
    "@vue/language-core": "^2.2.10",
    "bumpp": "^10.1.0",
    "diff": "^8.0.1",
    "eslint": "^9.26.0",
    "estree-walker": "^3.0.3",
    "prettier": "^3.5.3",
    "rolldown": "canary",
    "rollup-plugin-dts": "^6.2.1",
    "tinyglobby": "^0.2.13",
    "tsdown": "^0.11.9",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "resolutions": {
    "rolldown": "canary",
    "rolldown-plugin-dts": "workspace:*"
  },
  "prettier": "@sxzz/prettier-config"
}
