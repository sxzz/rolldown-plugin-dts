{
  "name": "rolldown-plugin-dts",
  "version": "0.17.3",
  "packageManager": "pnpm@10.20.0",
  "description": "A Rolldown plugin to generate and bundle dts files.",
  "type": "module",
  "keywords": [
    "rolldown",
    "plugin",
    "dts",
    "typescript",
    "vue",
    "jsdoc"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/rolldown-plugin-dts#readme",
  "bugs": {
    "url": "https://github.com/sxzz/rolldown-plugin-dts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/rolldown-plugin-dts.git"
  },
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/sxzz",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./filename": "./dist/filename.js",
    "./tsc": "./dist/tsc.js",
    "./tsc-context": "./dist/tsc-context.js",
    "./tsc-worker": "./dist/tsc-worker.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp",
    "prepack": "pnpm run build"
  },
  "peerDependencies": {
    "@ts-macro/tsc": "^0.3.6",
    "@typescript/native-preview": ">=7.0.0-dev.20250601.1",
    "rolldown": "^1.0.0-beta.44",
    "typescript": "^5.0.0",
    "vue-tsc": "~3.1.0"
  },
  "peerDependenciesMeta": {
    "@ts-macro/tsc": {
      "optional": true
    },
    "@typescript/native-preview": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "vue-tsc": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/types": "^7.28.5",
    "ast-kit": "^2.1.3",
    "birpc": "^2.6.1",
    "debug": "^4.4.3",
    "dts-resolver": "^2.1.2",
    "get-tsconfig": "^4.13.0",
    "magic-string": "^0.30.21"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@embroider/addon-dev": "^8.1.0",
    "@glimmer/component": "^2.0.0",
    "@glint/ember-tsc": "^1.0.7",
    "@rollup/plugin-babel": "^6.1.0",
    "@sxzz/eslint-config": "^7.2.8",
    "@sxzz/prettier-config": "^2.2.4",
    "@sxzz/test-utils": "^0.5.12",
    "@types/babel__generator": "^7.27.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^24.10.0",
    "@typescript/native-preview": "7.0.0-dev.20251102.1",
    "@volar/typescript": "^2.4.23",
    "@vue/language-core": "^3.1.2",
    "arktype": "^2.1.25",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "bumpp": "^10.3.1",
    "diff": "^8.0.2",
    "ember-source": "^6.8.1",
    "eslint": "^9.39.0",
    "estree-walker": "^3.0.3",
    "prettier": "^3.6.2",
    "rolldown": "^1.0.0-beta.45",
    "rolldown-plugin-require-cjs": "^0.3.1",
    "rollup-plugin-dts": "^6.2.3",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6",
    "vue": "^3.5.22",
    "vue-tsc": "^3.1.2"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "prettier": "@sxzz/prettier-config"
}
